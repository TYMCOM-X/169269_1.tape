      simple boolean procedure Get.Line;
      begin "Get.Line"
        Line _ null;
        reset(FF.Flag);
      
        while (true) do
        begin
          Line _ Line & input($$Chan(Src), Cpy.Tbl);
      
          if ($$Eof(Src)) then return(false);
      
          if ($$Brchr(Src) = ht) then
            Line _ Line & Blanks(Diff)
          else if ($$Brchr(Src) = ff) then
            set(FF.Flag)
          else if ($$Brchr(Src) = lf) then
          begin
            Inc(Line.Count);
            done;
          end;
        end;
    
        return(true);
      end "Get.Line";
    
    
