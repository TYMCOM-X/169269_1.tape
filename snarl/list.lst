      boolean procedure Title.Line;
      begin "Title.Line"
        integer Brk;
        string  Tmp;
    
        Title.Chr _ null;
    
        if (equ(Line[1 for 1], "!")) then
        begin
          Tmp _ Line;
      
          if (Starts.With(Tmp _ scan(Tmp, Blk.Tbl, Brk), "!title")) then
          begin
            Title.Chr _ "!";
            return(true);
          end
          else
            if (Starts.With(Tmp, "!!title")) then
            begin
              Title.Chr _ "!!";
              return(true);
            end;
        end
        else
          return(false);
      end "Title.Line";
    
    
